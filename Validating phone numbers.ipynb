   "source": [
    "# Match phone numbers starting with 7, 8, 9\r\n",
    "\r\n",
    "import re\r\n",
    "regex = r\"^[7-9]{1}[0-9]{9}$\"\r\n",
    "n = int(input())\r\n",
    "for _ in range(n):\r\n",
    "    ph = input()\r\n",
    "    if re.match(regex, ph):\r\n",
    "        print('YES')\r\n",
    "    else:\r\n",
    "        print('NO')"
   ],
   "outputs": [
    {
     "output_type": "stream",
     "name": "stdout",
     "text": [
      "Yes\n"
     ]
    }
   ],
   "metadata": {}
  }
